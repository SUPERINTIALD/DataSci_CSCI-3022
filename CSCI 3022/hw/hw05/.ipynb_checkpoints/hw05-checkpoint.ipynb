    "def sim(n):\n",
    "    count = 0\n",
    "    z = []\n",
    "    for i in range(n):\n",
    "        r1 = np.random.randint(1,7)\n",
    "        r2 = np.random.randint(1,7)\n",
    "        if((r1 == r2) or (r1 - r2) == 1 or (r2 - r1) == 1):\n",
    "           count += 1\n",
    "        z.append(count/(i+1))\n",
    "    result = count/n\n",
    "    return result,z\n",
    "\n",
    "sim(10000)\n",
    "#Your code above this line"
